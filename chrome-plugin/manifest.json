{
    "name": "Drift Annote",
    "version": "1.0",
    "description": "Chrome Extension to automate annotations. Built by Drift :)",
    "permissions": ["declarativeContent","activeTab","storage"],
    "background": {
        "scripts":["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/notepad16.png",
            "32": "images/notepad32.png",
            "48": "images/notepad48.png",
            "128": "images/notepad128.png"
        }
    },
    "icons": {
        "16": "images/notepad16.png",
        "32": "images/notepad32.png",
        "48": "images/notepad48.png",
        "128": "images/notepad128.png"
    },
    "manifest_version": 2
  }